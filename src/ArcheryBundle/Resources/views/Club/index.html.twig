{% extends "ArcheryBundle::layout.html.twig" %}

{% block title %}Cercle - {{ parent() }}{% endblock %}

{% block archery_content %}
    <h3>Liste des cercles</h3>
    <ul>
        {% for club in clubs %}
        <li>
            <a href="{{ path('club_view', {'slug': club.slug}) }}">{{ club.fullname }}</a>
        </li>
        {% endfor %}
    </ul>
{% endblock %}